{
  "name": "iubenda",
  "vendor": "vtex",
  "version": "0.2.3",
  "title": "Iubenda",
  "description": "Easy to implement solutions to make your website compliant with the law in various countries and laws.",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "builders": {
    "pixel": "0.x",
    "docs": "0.x"
  },
  "dependencies": {},
  "settingsSchema": {
    "title": "Iubenda",
    "type": "object",
    "properties": {
      "siteId": {
        "title": "Site ID",
        "type": "number"
      },
      "cookiePolicyId": {
        "title": "Cookie Policy ID",
        "type": "number"
      },
      "consentOnScroll": {
        "title": "Consent on scroll",
        "type": "boolean",
        "default": true
      },
      "banner": {
        "type": "object",
        "properties": {
          "position": {
            "title": "Position",
            "type": "string",
            "enum": [
              "top",
              "bottom",
              "float-top-left",
              "float-top-right",
              "float-bottom-left",
              "float-bottom-right",
              "float-top-center",
              "float-bottom-center",
              "float-center"
            ],
            "enumNames": [
              "Top",
              "Bottom",
              "Float top left",
              "Float top right",
              "Float bottom left",
              "Float bottom right",
              "Float top center (default)",
              "Float bottom center",
              "Float center"
            ],
            "default": "float-top-center"
          },
          "rejectButtonDisplay": {
            "title": "Display \"Reject\" button",
            "type": "boolean",
            "default": false
          }
        }
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
